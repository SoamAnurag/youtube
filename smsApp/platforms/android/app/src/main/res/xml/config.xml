<?xml version='1.0' encoding='utf-8'?>
<widget id="com.smsRecive.dev" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="SMSReceive">
        <param name="android-package" value="com.andreszs.smsreceive.SMSReceive" />
    </feature>
    <name>SOME THING</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="loglevel" value="DEBUG" />
    <feature name="SMSReader">
        <param name="android-package" value="io.github.rajeevs1992.smsreader.SMSReader" />
    </feature>
</widget>
